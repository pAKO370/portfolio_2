@keyframes flip
	0%
		opacity: .1
		transform: rotateY(90deg)
	100%
		opacity: .9
		transform: rotateY(0deg)

@keyframes flip-away
	0%
		opacity: .9
		transform: rotateY(0deg)
	100%
		opacity: 0
		transform: rotateY(90deg)

	
